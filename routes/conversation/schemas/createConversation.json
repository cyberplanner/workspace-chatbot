{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"properties": {
		"children": {
			"items": {
				"properties": {
					"intentId": {
						"type": "string"
					},
					"nodeId": {
						"type": "string"
					},
					"conditions": {
						"items": {
							"properties": {
								"entityId": {
									"type": "string"
								},
								"not": {
									"type":"boolean"
								},
								"comparator": {
									"type": "string",
									"enum": [
										"EQUALS",
										"CONTAINS",
										"REGEX_MATCH"
									]
								},
								"value": {
									"type": "string"
								}
							},
							"required": [
								"entityId",
								"comparator",
								"value",
								"not"
							],
							"type": "object"
						},
						"type": "array"
					}
				},
				"required": [
					"intentId",
					"nodeId"
				],
				"type": "object"
			},
			"type": "array"
		},
        "supercharger" : {
            "id": {
              "type":"string"  
            },
            "arguments":{
                "items":{
                    "type": "object"
                },
                "type": "array"
            },
            "message": {
                "type": "object"
		    },
            "required" : [
                "id",
                "arguments",
                "message"
            ],
            "type": "object"
        },
		"fallback": {
			"items": {
				"type": "string"
			},
			"type": "array"
		},
		"message": {
			"type": "string"
		}
	},
	"required": [
		"fallback",
		"message",
		"children"
	],
	"type": "object"
}