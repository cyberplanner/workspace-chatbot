{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "children": {
      "items": {
        "properties": {
          "intentId": {
            "type": "string"
          },
          "nodeId": {
            "type": "string"
          },
          "conditions": {
            "items": {
              "properties": {
                "entityId": {
                  "type": "string"
                },
                "not": {
                  "type": "boolean"
                },
                "caseSensitive": {
                  "type": "boolean"
                },
                "comparator": {
                  "type": "string",
                  "enum": ["EQUALS", "CONTAINS", "REGEX_MATCH"]
                },
                "value": {
                  "type": "string"
                },
                "checkUserData": {
                  "type": "boolean"
                }
              },
              "required": [
                "entityId",
                "comparator",
                "value",
                "not",
                "caseSensitive",
                "checkUserData"
              ],
              "type": "object"
            },
            "type": "array"
          }
        },
        "required": ["intentId", "nodeId"],
        "type": "object"
      },
      "type": "array"
    },
    "supercharger": {
      "properties": {
        "id": {
          "type": "string"
        },
        "arguments": {
          "type": "object"
        }
      },
      "required": ["id", "arguments"],
      "type": "object"
    },
    "fallback": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "message": {
      "type": "string"
    }
  },
  "required": ["fallback", "message", "children"],
  "type": "object"
}
